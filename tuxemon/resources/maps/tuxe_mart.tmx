  <object id="37" name="Talk to the professor" type="event" x="112" y="128" width="16" height="16">
   <properties>
    <property name="act1" value="dialog Professor: \n Ah\, yes. The very person I've been wanting to meet. "/>
    <property name="act2" value="dialog I've heard that you want to learn about Tuxemon."/>
    <property name="act3" value="dialog Tuxemon are little animals\, some call them monsters\, that we befriend and use to battle other Tuxemon."/>
    <property name="act4" value="dialog I was wondering\, I just received three new Tuxemon for research and I would like to give you one."/>
    <property name="act5" value="dialog Which one would you like?"/>
    <property name="act6" value="dialog_choice Hydrone:Rockitten:Fruitera,tuxchoice"/>
    <property name="cond1" value="is player_at"/>
    <property name="cond2" value="is player_facing up"/>
    <property name="cond3" value="is button_pressed K_RETURN"/>
    <property name="cond4" value="is npc_exists npc_professor"/>
    <property name="cond5" value="is variable_set tuxchoice:welcome"/>
   </properties>
  </object>
  <object id="42" name="hello Professor" type="event" x="160" y="176" width="16" height="16">
   <properties>
    <property name="act1" value="create_npc npc_professor,7,7,professor,stand"/>
    <property name="act2" value="set_variable tuxchoice:welcome"/>
    <property name="cond1" value="is player_at"/>
    <property name="cond2" value="is button_pressed K_RETURN"/>
    <property name="cond3" value="not variable_set tuxchoice:end"/>
    <property name="cond4" value="not npc_exists npc_professor"/>
    <property name="cond5" value="not variable_set tuxchoice:Hydrone"/>
    <property name="cond6" value="not variable_set tuxchoice:Rockitten"/>
    <property name="cond7" value="not variable_set tuxchoice:Fruitera"/>
   </properties>
  </object>
  <object id="46" name="choiceHydrone" type="event" x="112" y="128" width="16" height="16">
