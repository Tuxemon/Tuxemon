msgid "spyder_searoutec_rutherford1"
msgstr "De l'eau, de l'eau partout, ça fait vraiment réfléchir !"

msgid "spyder_searoutec_rutherford2"
msgstr "... mais pas une goutte à boire."

msgid "spyder_searoutec_carstair2"
msgstr "Je n'arrivais pas à garder ma tête en dehors de l'eau."

msgid "spyder_searoutec_sandy2"
msgstr "Je me suis laissé prendre au piège."

msgid "spyder_dragonscave_blocked"
